    "@nuxt/ui": "3.1.1",
    "@pinia/nuxt": "0.11.0",
    "@tailwindcss/vite": "^4.1.6",
    "@vueuse/nuxt": "13.1.0",
    "nuxt": "^3.17.2",
    "pinia": "^3.0.2",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.8.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.3.0",
    "@playwright/test": "1.52.0",
    "@tailwindcss/language-server": "^0.14.16",
    "@vitest/coverage-v8": "^3.1.3",
